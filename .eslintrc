{
  "parser": "@typescript-eslint/parser",
  "plugins": [ "import", "@typescript-eslint"],
  "extends": [
    "airbnb-base/legacy",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": ["error", 4]
  }
}
